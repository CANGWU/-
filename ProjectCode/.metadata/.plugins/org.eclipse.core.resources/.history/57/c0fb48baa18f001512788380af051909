package RMItest;

import java.rmi.Naming;
import dataservice.DataFactoryService;
import dataservice.PManagementDataService;
import po.StaffPO;

public class TestMain {
	
	public static void main(String[]args){
		DataFactoryService dataFactory;
		
		try{
			dataFactory = (DataFactoryService)Naming.lookup("//localhost:8000/getDataFactory");
			PManagementDataService pmanagementData = dataFactory.getPManagementData();
			StaffPO po = pmanagementData.find("1");
			System.out.println(po.getName());
			//System.out.println(dataFactory.getPManagementData().find("1").getName());
			
			
		}catch(Exception e){
			e.printStackTrace();
		}
		
		
		
	}

}

	public static PManagementDataImpl create() throws RemoteException{
		if(pm == null){
			synchronized(PManagementDataImpl.class){
		
		if(pm == null)
		pm = new PManagementDataImpl();
			}
		}
			
		return pm;
	}
	
   private volatile static PManagementDataImpl pm;

}
